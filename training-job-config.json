{
    "TrainingJobName": "your-training-job-name",
    "AlgorithmSpecification": {
        "TrainingImage": "683313688378.dkr.ecr.us-west-2.amazonaws.com/sagemaker-scikit-learn:0.23-1-cpu-py3",  # Scikit-Learn container URI
        "TrainingInputMode": "File"
    },
    "RoleArn": "arn:aws:iam::YOUR_ACCOUNT_ID:role/YOUR_SAGEMAKER_ROLE",  # Replace with your SageMaker IAM role
    "InputDataConfig": [
        {
            "ChannelName": "training",
            "DataSource": {
                "S3DataSource": {
                    "S3DataType": "S3Prefix",
                    "S3Uri": "s3://your-bucket-name/data/",  # Replace with your S3 data location
                    "S3DataDistributionType": "FullyReplicated"
                }
            },
            "ContentType": "text/csv"
        }
    ],
    "OutputDataConfig": {
        "S3OutputPath": "s3://your-bucket-name/model/"  # Replace with your S3 output location
    },
    "ResourceConfig": {
        "InstanceType": "ml.m5.large",
        "InstanceCount": 1,
        "VolumeSizeInGB": 10
    },
    "StoppingCondition": {
        "MaxRuntimeInSeconds": 3600
    }
}
